{
    "name": "@mark/admin",
    "version": "0.0.1",
    "description": "Admin logic for Mark.",
    "license": "MIT",
    "author": "Everclear",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "src/**/*"
    ],
    "scripts": {
        "build": "tsc --build ./tsconfig.json",
        "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
        "dev": "ts-node-dev -r tsconfig-paths/register --respawn src/dev.ts",
        "lint": "eslint src",
        "lint:fix": "yarn lint --fix",
        "test": "yarn test:unit",
        "test:unit": "jest --coverage"
    },
    "dependencies": {
        "@mark/cache": "workspace:*",
        "@mark/core": "workspace:*",
        "@mark/logger": "workspace:*",
        "aws-lambda": "1.0.7",
        "datadog-lambda-js": "10.123.0",
        "dd-trace": "5.42.0",
        "viem": "2.33.3"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.147",
        "@types/jest": "29.5.12",
        "@types/node": "20.17.12",
        "eslint": "9.17.0",
        "jest": "29.7.0",
        "rimraf": "6.0.1",
        "ts-jest": "29.1.2",
        "ts-node": "10.9.2",
        "ts-node-dev": "2.0.0",
        "typescript": "5.7.2"
    }
}
