{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "reporter": [
        "text",
        "html"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/types/*.ts",
        "src/errors.ts",
        "test/**/*.spec.ts"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
}