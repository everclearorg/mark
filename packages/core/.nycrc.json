{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "reporter": [
        "text",
        "html"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/**/*.spec.ts",
        "src/**/*.d.ts"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
}